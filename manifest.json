{
  "manifest_version": 2,
  "name": "fuzzy-find",
  "description": "Fuzzy search your bookmarks",
  "version": "0.1.2",
  "browser_action": {
    "default_icon": "assets/icon.png",
    "default_popup": "src/popup-page/popup.html",
    "default_title": "fuzzy-find search box"
  },
  "permissions": [
    "bookmarks"
  ]
}
