{
  "manifest_version": 2,
  "name": "fuzzy-find",
  "description": "Fuzzy search anything on a webpage",
  "version": "0.0.1",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "fuzzy-find search box"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["lib/mark.min.js", "dist/content.js"]
    }
  ]
}
